{
  "name": "offhand",
  "version": "0.0.1",
  "description": "Offhand is a fake data generator for JavaScript",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "mocha --parallel",
    "test:build": "parcel build src/index.ts && mocha --parallel",
    "build": "parcel build src/index.ts",
    "check": "tsc --noEmit",
    "check:format": "prettier -c src/**",
    "dev": "parcel watch src/index.ts",
    "lint": "eslint src/**"
  },
  "keywords": [
    "random",
    "random data",
    "data",
    "mock data",
    "fake",
    "fake data",
    "data generator"
  ],
  "author": "Erin Burton",
  "license": "MIT",
  "devDependencies": {
    "@parcel/core": "^2.0.1",
    "@parcel/packager-ts": "^2.0.1",
    "@parcel/transformer-typescript-types": "^2.0.1",
    "@types/node": "^17.0.6",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "chai": "^4.3.4",
    "eslint": "^8.6.0",
    "mocha": "^9.1.3",
    "parcel": "^2.0.1",
    "typescript": "^4.5.4"
  }
}
